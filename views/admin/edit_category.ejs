<%- include('../partials/adminHeader'); %>
<div class="container input-fields">
    <br>
    <a href="/admin/categories" class="waves-effect waves-teal btn-flat btn-floating "><i class="material-icons icon-grey">arrow_back</i></a>
    <span class="page-title">Edit page</span>
    <br>
    <br>
    <form method="POST" action="/admin/categories/edit-category/<%= id %>">
        <div class="input-field form-group">
            <i class="material-icons prefix">title</i>
            <input id="icon_prefix" type="text" name="title" class="form-control" value="<%= title %>" autocomplete="off">
            <label for="icon_prefix">Title</label>
        </div>
        

        <div class="fixed-action-btn">
            <button class=" btn-floating btn-large waves-effect waves-light deep-purple tooltipped" data-position="left"
                data-tooltip="Save"><i class="material-icons">save</i>Save</button>
        </div>
        
        </div>
    </form>
</div>


<%- include('../partials/adminFooter'); %>